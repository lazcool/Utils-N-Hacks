							"structure": [
								{
									"name": "Id",
									"type": "Int64"
								},
								{
									"name": "HashedId",
									"type": "String"
								},
								{
									"name": "PublicHashedId",
									"type": "String"
								},
								{
									"name": "Name",
									"type": "String"
								},
								{
									"name": "Created",
									"type": "DateTime"
								},
								{
									"name": "Updated",
									"type": "DateTime"
								}
							],
							"tableName": "[dbo].[Accounts]"
						},
						"copyActivity": {
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"Id": "Id",
									"HashedId": "HashedId",
									"PublicHashedId": "PublicHashedId",
									"Name": "Name",
